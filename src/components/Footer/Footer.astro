---
import type { IFooter } from '@/interfaces/home-page';
import {FooterContactToggle} from '@/components/Footer/FooterContactToggle.tsx';

interface Props {
  t: IFooter
}

const {t} = Astro.props
 
---
 <section class='mb-10 '>
        <div  class='text-center'>
          <h5 class='sm:text-[18px] text-[14px] text-secondary uppercase tracking-wider'>
            {t.uppertitle}
          </h5>
         <FooterContactToggle t={t} client:visible/>
        </div>
     
    </section>